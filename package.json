{
  "name": "Nand2Tetris-vscode",
  "displayName": "Nand2Tetris For Vscode",
  "description": "Supports snippets and linting for Nand2Tetris Courses,Snippets nand2tetris hdl, tst, cmp and asm  files, lanuage support and syntax-highlighting for hdl, tst, cmp, asm and out files",
  "version": "0.3.0",
  "engines": {
    "vscode": "^1.8.0"
  },
  "publisher": "Loyio",
  "author": {
    "name": "Loyio",
    "url": "https://github.com/loyio"
  },
  "icon": "images/nand2tetris.png",
  "keywords": [
    "hdl",
    "tst",
    "asm",
    "out",
    "cmp",
    "hack",
    "jack",
    "nand2tetris",
    "snippets",
    "syntax-highlighting",
    "syntaxhighlighting",
    "language support"
  ],
  "license": "MIT",
  "homepage": "https://github.com/loyio/Nand2Tetris-vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/loyio/Nand2Tetris-vscode"
  },
  "bugs": {
    "url": "https://github.com/loyio/Nand2Tetris-vscode/issues"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Extension Packs",
    "Language Packs"
  ],
  "contributes": {
    "languages": [
      {
        "id": "hdl",
        "aliases": [
          "HDL",
          "hdl"
        ],
        "extensions": [
          ".hdl"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "tst",
        "aliases": [
          "TST",
          "tst"
        ],
        "extensions": [
          ".tst"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "cmp",
        "aliases": [
          "CMP",
          "cmp"
        ],
        "extensions": [
          ".cmp"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "out",
        "aliases": [
          "OUT",
          "out"
        ],
        "extensions": [
          ".out"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "asm",
        "aliases": [
          "ASM",
          "asm"
        ],
        "extensions": [
          ".asm"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "hack",
        "aliases": [
          "HACK",
          "hack"
        ],
        "extensions": [
          ".hack"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "jack",
        "aliases": [
          "JACK",
          "jack"
        ],
        "extensions": [
          ".jack"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "hdl",
        "scopeName": "source.hdl",
        "path": "./syntaxes/hdl.tmLanguage.json"
      },
      {
        "language": "tst",
        "scopeName": "source.tst",
        "path": "./syntaxes/tst.tmLanguage.json"
      },
      {
        "language": "cmp",
        "scopeName": "source.cmp",
        "path": "./syntaxes/cmp.tmLanguage.json"
      },
      {
        "language": "out",
        "scopeName": "source.out",
        "path": "./syntaxes/out.tmLanguage.json"
      },
      {
        "language": "asm",
        "scopeName": "source.asm",
        "path": "./syntaxes/asm.tmLanguage.json"
      },
      {
        "language": "hack",
        "scopeName": "source.hack",
        "path": "./syntaxes/hack.tmLanguage.json"
      },
      {
        "language": "jack",
        "scopeName": "source.jack",
        "path": "./syntaxes/jack.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "hdl",
        "path": "./snippets/hdl-files/Elementary-logic-gates.code-snippets"
      },
      {
        "language": "hdl",
        "path": "./snippets/hdl-files/Elementary-logic-gates-16Bit.code-snippets"
      },
      {
        "language": "hdl",
        "path": "./snippets/hdl-files/Combinational-chips.code-snippets"
      },
      {
        "language": "hdl",
        "path": "./snippets/hdl-files/Sequential-chips.code-snippets"
      },
      {
        "language": "hdl",
        "path": "./snippets/hdl-files/Computer-Architecture.code-snippets"
      },
      {
        "language": "tst",
        "path": "./snippets/tst-files/Test-files.code-snippets"
      },
      {
        "language": "tst",
        "path": "./snippets/tst-files/Test-files-2var.code-snippets"
      },
      {
        "language": "tst",
        "path": "./snippets/tst-files/Test-files-3var.code-snippets"
      },
      {
        "language": "cmp",
        "path": "./snippets/cmp-files/Cmp-files-rows.code-snippets"
      },
      {
        "language": "cmp",
        "path": "./snippets/cmp-files/Cmp-files-heads.code-snippets"
      },
      {
        "language": "cmp",
        "path": "./snippets/cmp-files/Cmp-files-tables.code-snippets"
      },
      {
        "language": "asm",
        "path": "./snippets/asm-files/Asm-files-branching.code-snippets"
      },
      {
        "language": "jack",
        "path": "./snippets/jack-files/Jack-files-class.code-snippets"
      },
      {
        "language": "jack",
        "path": "./snippets/jack-files/Jack-files-datatype.code-snippets"
      },
      {
        "language": "jack",
        "path": "./snippets/jack-files/Jack-files-branching.code-snippets"
      }
    ]
  }
}
